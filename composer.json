{
    "name": "undeadyetii/rhymegenerator",
    "description": "Generates rhymes with little to moderate user input",
    "license": "MIT",
    "authors": [
        {
            "name": "UnDeAdYeTii",
            "homepage": "https://github.com/UnDeAdYeTii/",
            "role": "Developer"
        }
    ],
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1",
        "undeadyetii/libraries": "^0.5",
        "inouet/file-cache": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "YeTii\\RhymeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YeTii\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    }
}
